{
  "name": "feathersreactnativechat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "mocha --require test/mocks --compilers js:babel-core/register --recursive test/specs/**/*.spec.js",
    "watch:test": "nodemon -w test -e js,jsx -x npm test",
    "test:integration": "npm run mocha-test test/integration",
    "mocha-test": "mocha --recursive --compilers js:babel/register",
    "compile:test": "TARGET=test node tasks/compile.js",
    "compile:staging": "TARGET=staging node tasks/compile.js",
    "install:staging": "PHONE=true TARGET=staging node tasks/compile.js"
  },
  "dependencies": {
    "events": "^1.1.0",
    "feathers": "2.0.0",
    "feathers-authentication": "^0.4.1",
    "feathers-hooks": "^1.3.0",
    "feathers-localstorage": "^0.2.0",
    "feathers-socketio": "^1.3.3",
    "react-native": "^0.20.0",
    "react-native-drawer": "^1.16.7",
    "react-native-gifted-messenger": "corymsmith/react-native-gifted-messenger",
    "react-native-router-flux": "2.3.1",
    "react-native-vector-icons": "^1.2.1",
    "socket.io-client": "^1.4.5"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.6.4",
    "babel-jest": "^6.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-airbnb": "^1.0.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-native": "^1.4.0",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "cheerio": "^0.20.0",
    "enzyme": "^2.0.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-native-mock": "^0.0.6",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0"
  },
  "babel": {
    "retainLines": true,
    "compact": true,
    "comments": false,
    "presets": [
      "es2015",
      "stage-0",
      "react",
      "react-native"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "ignore" : "/node_modules\/(?!react-native-router-flux)/",
    "sourceMaps": false
  }
}
